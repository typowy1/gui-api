<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="All Gui Tests" parallel="tests" thread-count="1">

    <listeners>
        <listener class-name="pl.example.gui.utils.TestListener"/>
    </listeners>

    <test name="Top Menu E2E Tests">
        <!--        <parameter name="browser" value="CHROME"/>-->
        <classes>
            <class name="pl.example.gui.tests.e2e.TopMenuE2ETests"/>
        </classes>
    </test>

    <test name="Cookie preference E2E Tests">
        <!--        <parameter name="browser" value="CHROME"/>-->
        <classes>
            <class name="pl.example.gui.tests.e2e.CookiePreferenceE2ETests"/>
        </classes>
    </test>

    <test name="Footer E2E Tests">
        <!--        <parameter name="browser" value="CHROME"/>-->
        <classes>
            <class name="pl.example.gui.tests.e2e.FooterE2ETests"/>
        </classes>
    </test>

</suite>